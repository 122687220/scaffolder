{
  "name": "scaffolder-cli",
  "version": "1.4.4",
  "bin": {
    "scaff": "./cli.js"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/tests/e2e/results",
      "results/*"
    ]
  },
  "description": "Dynamically create predefined templates",
  "main": "index.js",
  "preversion": "npm test",
  "scripts": {
    "preversion": "npm test",
    "postpublish": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "Gal ELmalah",
  "license": "ISC",
  "devDependencies": {
    "jest": "^24.1.0",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^2.19.0",
    "fs-extra": "^9.0.1",
    "inquirer": "^6.5.0"
  },
  "keywords": [
    "cli",
    "generator",
    "templates"
  ],
  "bugs": {
    "url": "https://github.com/galElmalah/scaffolder/issues"
  },
  "homepage": "https://github.com/galElmalah/scaffolder",
  "repository": {
    "type": "git",
    "url": "https://github.com/galElmalah/scaffolder"
  }
}
